# Exemplo de configuração de ambiente para desenvolvimento local
# Copie este arquivo para .env e ajuste os valores

# Nível de log (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO

# Região AWS padrão
AWS_DEFAULT_REGION=us-east-1

# Credenciais AWS (alternativa ao aws configure)
# AWS_ACCESS_KEY_ID=your_access_key
# AWS_SECRET_ACCESS_KEY=your_secret_key
# AWS_SESSION_TOKEN=your_session_token  # Para roles temporárias

# Configurações opcionais
# PYTHONPATH=src  # Para execução local